{
	"name": "ortzinator/classie",
	"description": "Classie Classifieds",
	"license": "MIT",
	"authors": [
		{
			"name": "Ortzinator",
			"email": "ortzinator@gmail.com"
		}
	],
	"keywords": ["framework", "laravel"],
	"require": {
		"laravel/framework": "4.2.11",
		"cartalyst/sentry": "2.1.*",
		"vtalbot/markdown": "1.*",
		"laravelbook/ardent": "2.*",
		"philf/setting": "dev-master",
		"mccool/laravel-auto-presenter": "*",
		"intervention/image": "dev-master",
		"itsgoingd/clockwork": "1.*"
	},
	"require-dev": {
		"phpunit/phpunit": "5.7.*",
		"mockery/mockery": "dev-master@dev",
		"way/generators": "~2.0",
		"fzaninotto/faker": "~1.4",
		"barryvdh/laravel-debugbar": "~1.8",
		"laracasts/integrated": "^0.15.6"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"tests/TestCase.php"
		],
		"psr-0": {
			"Ortzinator": "app/lib"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
